# Bot Framework Variables
MICROSOFT_APP_ID=
MICROSOFT_APP_PASSWORD=

# This Url can be obtained by uploading or creating your model from the LUIS portal: https://www.luis.ai/
LUIS_MODEL_URL=https://westus.api.cognitive.microsoft.com/luis/v2.0/apps/101a6522-b9de-4faf-b47e-91bd412a669a?subscription-key=88a14a9b08e24a57949536403ecb2814&verbose=true&timezoneOffset=0&q=

